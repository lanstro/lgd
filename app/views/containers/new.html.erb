<% provide(:title, 'Create new container') %>
<h1>Create new container</h1>

<div class="row">
  <div class="span6 offset1">
    <%= form_for(@container) do |f| %>
			<%= render 'form', f: f, errors: @container.errors %>
      <%= f.submit "Create new container", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>