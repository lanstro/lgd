<% tags = Act.html_tags(container) %>
<% if tags %>
  <%= raw tags[0] %>
<% end %>

<%= container.content %>

<% if container.children.size > 0 %>
		<% container.children.each do |container| %>
			<%= render container %>
		<% end %>
<% end %>

<% if tags %>
  <%= raw tags[1] %>
<% end %>